{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true
  },
  "markdownlint.config": {
    "default": true,
    "no-inline-html": { "allowed_elements": ["img"] },
    "ul-indent": { "indent": 2 }
  },
  "explorer.sortOrder": "mixed",
  "explorer.sortOrderLexicographicOptions": "default"
}
